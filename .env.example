# Environment Variables
# Copy this file to .env and fill in your values

# Pluggy API credentials
PLUGGY_CLIENT_ID=your_pluggy_client_id
PLUGGY_CLIENT_SECRET=your_pluggy_client_secret
PLUGGY_CONNECTOR_ID=your_pluggy_connector_id

# Maybe.co API credentials and settings
MAYBE_APP_URL=https://your-maybe-app-url.com
MAYBE_EMAIL=your-email@example.com
MAYBE_PASSWORD=your_maybe_password
MAYBE_CHECKING_ACCOUNT_ID=your_maybe_checking_account_id

# Database connection
DATABASE_URL=postgres://username:password@localhost:5432/your_database